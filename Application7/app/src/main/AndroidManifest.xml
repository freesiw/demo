<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.application7">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FlashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.application7.AboutActivity"/>
        <activity android:name="com.example.administrator.application7.CollectorActivity"/>
        <activity android:name="com.example.administrator.application7.DetailsActivity"/>
        <activity android:name="com.example.administrator.application7.FedbackActivity"/>
        <activity android:name="com.example.administrator.application7.HomeActivity"/>
        <activity android:name="com.example.administrator.application7.SettingsActivity"/>
        <activity android:name=".WelcomeActivity"/>

    </application>

</manifest>
